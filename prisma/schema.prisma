generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Evento {
  id          String  @id @default(auto()) @map("_id") @db.ObjectId
  bairro      String
  cidade      String
  complemento String
  criadorId   String?
  data        String
  descricao   String
  endereco    String
  horario     String
  imagem      String
  local       String
  maxPessoa   String?
  nome        String
  numero      String
  tipo        String
  tipo_evento String?
  uf          String
}

model Usuario {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  /// Field referred in an index, but found no data to define the type.
  email Json?  @unique
}
